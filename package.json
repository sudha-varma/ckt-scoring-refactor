{
  "name": "crictecScoring",
  "version": "0.0.1",
  "description": "crictecScoring",
  "author": "",
  "main": "server.js",
  "private": true,
  "engines": {
    "node": ">=8.9.0",
    "npm": ">=5.5.0",
    "yarn": ">=1.2.1"
  },
  "scripts": {
    "start2": "export NODE_ENV=development;babel-node ./server.js",
    "lint": "esw *.js server config --color",
    "lint:watch": "npm run lint -- --watch",
    "test": "cross-env NODE_ENV=test ./node_modules/.bin/mocha --compilers js:babel-core/register --ui bdd --reporter spec --colors server --recursive",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "cross-env NODE_ENV=test ./node_modules/.bin/istanbul cover _mocha -- --compilers js:babel-core/register --ui bdd --reporter spec --colors server --recursive",
    "test:check-coverage": "npm run test:coverage && istanbul check-coverage",
    "report-coverage": "coveralls < ./coverage/lcov.info",
    "postinstall": "./bin/symLinkHooks.sh",
    "checkGitHooks": "ls -la .git/hooks/ | grep '\\->'",
    "start": "export NODE_ENV=development;nodemon --legacy-watch -w . --exec \"babel-node ./server.js\"",
    "startP": "export NODE_ENV=production;nodemon --legacy-watch -w . --exec \"babel-node ./server.js\"",
    "startT": "export NODE_ENV=test;nodemon --legacy-watch -w . --exec \"babel-node ./server.js\"",
    "start:debug": "export NODE_ENV=development;nodemon --inspect-brk=0.0.0.0 --legacy-watch -w . --exec \"babel-node ./server.js\"",
    "clean": "rm -rf dist && mkdir dist",
    "build": "babel . -D -d dist --ignore node_modules/,CONTRIBUTING.md,docker-compose.yml,docker-compose.dev.yml,docker-compose.production.yml,docker-compose.test.yml,README.md,Dockerfile,LICENSE,.travis.yml,log/,tests/,.DS_Store,package-lock.json",
    "build:start": "npm run clean; npm run build;npm run start:prod",
    "start:prod": "export NODE_ENV=development;node dist/server.js",
    "eslintFix": "eslint --fix ."
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [],
  "dependencies": {
    "aws-sdk": "2.299.0",
    "bluebird": "3.5.1",
    "body-parser": "1.18.2",
    "compression": "1.7.2",
    "cookie-parser": "1.4.3",
    "cors": "2.8.4",
    "dotenv": "6.0.0",
    "express": "4.16.3",
    "helmet": "3.12.0",
    "http-status": "1.0.1",
    "joi": "13.6.0",
    "lodash": "4.17.10",
    "method-override": "3.0.0",
    "mongoose": "5.3.2",
    "morgan": "1.9.0",
    "redis": "2.8.0",
    "sha1": "1.1.1",
    "uuid": "3.3.2",
    "winston": "3.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "9.0.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-airbnb": "2.6.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-es2015": "6.24.1",
    "babel-register": "6.26.0",
    "chai": "4.1.2",
    "coveralls": "3.0.0",
    "cross-env": "5.1.4",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "5.5.0",
    "eslint-config-airbnb-base": "13.0.0",
    "eslint-config-prettier": "3.0.1",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-prettier": "2.6.0",
    "istanbul": "1.1.0-alpha.1",
    "mocha": "5.2.0",
    "nodemon": "1.9.2",
    "prettier": "1.12.1",
    "supertest": "3.1.0"
  },
  "license": "",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
